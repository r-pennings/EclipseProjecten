
public class Ship {

	private String type;
	private int length;
	private int countShots;

	public Ship(String type, int length) {
		this.type = type;
		this.length = length;
	}

	public String getType() {
		return type;
	}

	public int getLength() {
		return length;
	}

	public int getCountShots() {
		return countShots;
	}
}
